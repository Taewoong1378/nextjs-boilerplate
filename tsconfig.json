{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "preserve",
    "strict": true,
    "module": "commonjs",
    "target": "es6",
    "typeRoots": ["node_modules/@types", "./types"],
    "baseUrl": ".",
    "paths": {
      "@components": ["components"],
      "@components/*": ["components/*"],
      "@atoms/*": ["components/atoms/*"],
      "@molecules/*": ["components/molecules/*"],
      "@organisms/*": ["components/organisms/*"],
      "@templates": ["templates"],
      "@templates/*": ["templates/*"],
      "@constants": ["constants"],
      "@constants/*": ["constants/*"],
      "@config/*": ["config/*"],
      "@assets": ["assets"],
      "@assets/*": ["assets/*"],
      "@hooks": ["hooks"],
      "@hooks/*": ["hooks/*"],
      "@styles": ["styles"],
      "@styles/*": ["styles/*"],
      "@types/*": ["types/*"],
      "@themes": ["themes"],
      "@themes/*": ["themes/*"],
      "@util": ["util"],
      "@util/*": ["util/*"],
      "@pages/*": ["pages/*"]
    },
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/organisms/Map/index.jsx",
    "components/organisms/Map/index.jsx",
    "components/organisms/Map/index.js",
    "components/organisms/Map/index.js"
  ],
  "exclude": ["node_modules"]
}
